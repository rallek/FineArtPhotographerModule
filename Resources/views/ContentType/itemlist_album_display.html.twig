{# Purpose of this template: Display albums within an external context #}
{% for album in items %}
    <h3>{{ album|rkfineartphotographermodule_formattedTitle }}</h3>
    <p><a href="{{ path('rkfineartphotographermodule_album_display', {'id': album.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
